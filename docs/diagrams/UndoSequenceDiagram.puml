@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":XpireParser" as XpireParser LOGIC_COLOR
participant "u:UndoCommand" as UndoCommand LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
participant ":VersionedXpire" as VersionedXpire MODEL_COLOR
end box
[-> LogicManager : execute(undo)
activate LogicManager

LogicManager -> XpireParser : parseCommand(undo)
activate XpireParser

create UndoCommand
XpireParser -> UndoCommand
activate UndoCommand

UndoCommand --> XpireParser
deactivate UndoCommand

XpireParser --> LogicManager : u
deactivate XpireParser

LogicManager -> UndoCommand : execute()
activate UndoCommand

UndoCommand -> Model : undoXpire()
activate Model

Model -> VersionedXpire : undo()
activate VersionedXpire

VersionedXpire -> VersionedXpire :resetData(ReadOnlyXpire)
VersionedXpire --> Model :
deactivate VersionedXpire

Model --> UndoCommand
deactivate Model

UndoCommand --> LogicManager : result
deactivate UndoCommand
UndoCommand -[hidden]-> LogicManager : result
destroy UndoCommand

[<--LogicManager
deactivate LogicManager
@enduml
